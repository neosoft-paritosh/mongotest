<h1>
    Courses
</h1>

<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Duration</th>
        <th>Fees</th>
    </tr>
    {{#each data}}
    <tr>
        <td>{{this.courseId}}</td>
        <td>{{this.courseName}}</td>
        <td>{{this.courseDuration}}</td>
        <td>{{this.courseFees}}</td>
        
    </tr>
    {{/each}}
</table>